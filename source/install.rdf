<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
  
    <em:id>{1A2D0EC4-75F5-4c91-89C4-3656F6E44B68}</em:id>
    <em:name>Image Zoom</em:name>
    <em:version>0.2.2</em:version>
    <em:description>Adds zoom functionality for images</em:description>
    <em:creator>Jason Adams</em:creator>
    <em:homepageURL>http://imagezoom.yellowgorilla.net/</em:homepageURL>
    <em:optionsURL>chrome://imagezoom/content/options.xul</em:optionsURL>   
    <em:iconURL>chrome://imagezoom/skin/imagezoom.png</em:iconURL>   
    <em:aboutURL>chrome://imagezoom/content/about.xul</em:aboutURL>

    <em:file>
      <Description about="urn:mozilla:extension:file:imagezoom.jar">
        <em:package>content/</em:package>
        <em:locale>locale/en-US/</em:locale>
        <em:locale>locale/fr-FR/</em:locale>
        <em:locale>locale/ja-JP/</em:locale>
        <em:locale>locale/sl-SI/</em:locale>
        <em:locale>locale/zh-TW/</em:locale>
        <em:locale>locale/de-DE/</em:locale>
        <em:locale>locale/cs-CZ/</em:locale>
        <em:skin>skin/classic/imagezoom/</em:skin>
      </Description>
    </em:file>

    <!-- Firefox -->    
    <em:targetApplication>
     <Description>
       <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
       <em:minVersion>1.0</em:minVersion>
       <em:maxVersion>1.6</em:maxVersion>
     </Description>
    </em:targetApplication>    

    <!-- Thunderbird -->    
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>1.6</em:maxVersion>
      </Description>
    </em:targetApplication>   

    <!-- Flock -->
    <em:targetApplication>
      <Description>
        <em:id>{a463f10c-3994-11da-9945-000d60ca027b}</em:id>
        <em:maxVersion>1.0+</em:maxVersion>
        <em:minVersion>1.0+</em:minVersion>
      </Description>
    </em:targetApplication>
    
    
  </Description>      
</RDF>

